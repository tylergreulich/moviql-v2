    "@types/classnames": "^2.2.6",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.9.4",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-motion": "^0.0.27",
    "@types/react-router-dom": "^4.3.0",
    "@types/react-slick": "^0.23.2",
    "react-testing-library": "^5.0.1",
    "typescript": "^3.0.3"
  }
}
